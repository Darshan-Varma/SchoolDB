@model SchoolDB.Models.Teacher
@{
    ViewBag.Title = "List";
}

<a class="navlink" href="/Teacher/List">Back To Teachers</a>
<a class="navlink dellink" href="/Teacher/DeleteConfirm/@Model.TeacherId">Delete</a>
<a class="navlink updatelink" href="/Teacher/Update/@Model.TeacherId">Update</a>
<div id="containershow">
    <h2>@Model.TeacherFname @Model.TeacherLname</h2>
    <p>@Model.CourseName is taught by @Model.TeacherFname and the Hire Date of Teacher is @Model.HireDate and Salary is @Model.TeacherSalary and Employee code is @Model.EmployeeNumber.</p>
</div>
